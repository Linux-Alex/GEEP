<genetic_programming_config>
    <mutation_probability>0.05</mutation_probability>
    <reproduction_probability>0.2</reproduction_probability>
    <crossover_probability>0.7</crossover_probability>
    <population_size>100000</population_size>
    <max_tree_depth>5</max_tree_depth>
    <selection_method>tournament</selection_method>
    <terminals>
        <terminal>x</terminal>
    </terminals>
    <functions>
        <function>add</function>
        <function>subtract</function>
        <function>multiply</function>
        <function>divide</function>
        <function>power</function>
    </functions>
    <init_program>
        <function name="add">
            <function name="multiply">
                <terminal>x</terminal>
                <terminal>2</terminal>
            </function>
            <terminal>3</terminal>
        </function>
    </init_program>
    <fitness_cases>
        <fitness_case>
            <input>1</input>
            <output>5</output>
        </fitness_case>
        <fitness_case>
            <input>2</input>
            <output>7</output>
        </fitness_case>
        <fitness_case>
            <input>3</input>
            <output>9</output>
        </fitness_case>
        <fitness_case>
            <input>4</input>
            <output>11</output>
        </fitness_case>
        <fitness_case>
            <input>5</input>
            <output>13</output>
        </fitness_case>
    </fitness_cases>
    <fitness_function>
        <type>mean_squared_error</type>
    </fitness_function>
    <stop_conditions>
        <stop_condition>
            <type>max_generations</type>
            <value>10000</value>
        </stop_condition>
        <stop_condition>
            <type>fitness_threshold</type>
            <value>0.01</value>
        </stop_condition>
    </stop_conditions>
</genetic_programming_config>