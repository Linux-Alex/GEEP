<genetic_algorithm>
    <problem_type>symbolic_regression</problem_type>
    <problem_name>Find equation for Concrete Compressive Strength</problem_name>

    <stopping_criteria>
        <criterion type="generations">10000</criterion>
        <!-- <criterion type="mse">100.0</criterion> -->
    </stopping_criteria>

    <algorithm_parameters>
        <population_size>1000</population_size>
        <elitism_count>10</elitism_count>
        <max_depth>5</max_depth>
        <max_nodes>31</max_nodes>
    </algorithm_parameters>

    <selection_method>
        <type>tournament</type>
        <tournament_size>9</tournament_size>
    </selection_method>

    <crossover_method>
        <type>subtree_crossover</type>
        <reproduction_rate>0.02</reproduction_rate>
    </crossover_method>

    <mutation_method>
        <type>standard</type>
        <mutation_rate>0.5</mutation_rate>
    </mutation_method>

    <function_set>
        <function>add</function>
        <function>multiply</function>
        <function>divide</function>
        <function>subtract</function>
    </function_set>

    <terminal_set>
        <variable>Cement</variable>
        <variable>Blast Furnace Slag</variable>
        <variable>Fly Ash</variable>
        <variable>Water</variable>
        <variable>Superplasticizer</variable>
        <variable>Coarse Aggregate</variable>
        <variable>Fine Aggregate</variable>
        <variable>Age</variable>
        <constant/>
    </terminal_set>

    <execution_mode>gpu</execution_mode>
    <!-- execution_mode can be: cpu or gpu -->

    <target_data delimiter="," target_column="Concrete compressive strength">
